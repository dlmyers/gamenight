<h1>My Schedule</h1>

<% @events.each do |event| %>
    <div class="entry">
      <h3><%= link_to "#{event.name} taking place #{event.date.strftime('%B %-d at %-I:%M %p')}", event_path(event)  %></h3>
      <%= sanitize(event.description) %><br />
      <%= link_to event.location, "https://maps.google.com?q=#{event.location}" %>

    </div>
<% end %>